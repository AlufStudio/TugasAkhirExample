<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hardy.pakarapel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.activities.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activities.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activities.KonsultasiActivity" />
        <activity android:name=".views.activities.DiagnosaActivity" />
        <activity android:name=".views.activities.BantuanActivity" />
        <activity android:name=".views.activities.TentangActivity" />
        <activity android:name=".views.activities.FeedbackActivity" />
        <activity android:name=".views.activities.ListHamaActivity" />
        <activity android:name=".views.activities.BudiDayaActivity"></activity>
    </application>

</manifest>